{
    "name": "dhulbeeg-backend",
    "version": "1.0.0",
    "description": "Backend API for DhulBeeg Real Estate & Legal Services",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --verbose",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"**/*.{js,json,md}\"",
        "seed": "node scripts/seedDatabase.js",
        "migrate": "node scripts/migrateDatabase.js",
        "backup": "node scripts/backupDatabase.js",
        "production": "NODE_ENV=production node server.js",
        "docker:build": "docker build -t dhulbeeg-backend .",
        "docker:run": "docker run -p 5000:5000 dhulbeeg-backend"
    },
    "keywords": [
        "real-estate",
        "legal-services",
        "property",
        "hargeisa",
        "somaliland",
        "api"
    ],
    "author": "DhulBeeg Real Estate & Law",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "mongoose": "^7.0.0",
        "dotenv": "^16.0.3",
        "cors": "^2.8.5",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.0",
        "validator": "^13.9.0",
        "express-rate-limit": "^6.7.0",
        "express-mongo-sanitize": "^2.2.0",
        "xss-clean": "^0.1.4",
        "hpp": "^0.2.3",
        "compression": "^1.7.4",
        "express-fileupload": "^1.4.0",
        "multer": "^1.4.5-lts.1",
        "cloudinary": "^1.37.0",
        "sharp": "^0.32.0",
        "nodemailer": "^6.9.1",
        "handlebars": "^4.7.7",
        "redis": "^4.6.5",
        "socket.io": "^4.6.1",
        "axios": "^1.4.0",
        "moment": "^2.29.4",
        "crypto": "^1.0.1",
        "uuid": "^9.0.0",
        "speakeasy": "^2.0.0",
        "qrcode": "^1.5.3",
        "pdfkit": "^0.14.0",
        "exceljs": "^4.3.0",
        "stripe": "^12.10.0",
        "paypal-rest-sdk": "^1.8.1",
        "googleapis": "^118.0.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-google-oauth20": "^2.0.0",
        "passport-facebook": "^3.0.0",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.2",
        "winston": "^3.9.0",
        "winston-daily-rotate-file": "^4.7.1",
        "joi": "^17.9.2",
        "express-validator": "^6.15.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.22",
        "jest": "^29.5.0",
        "supertest": "^6.3.3",
        "eslint": "^8.38.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "prettier": "^2.8.8",
        "faker": "^5.5.3",
        "cross-env": "^7.0.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dhulbeeg/backend-api.git"
    },
    "bugs": {
        "url": "https://github.com/dhulbeeg/backend-api/issues"
    },
    "homepage": "https://dhulbeeg.com/api-docs"
}